    featProp Clover.Costume.glow setTo 1

  ]]

  // üîé Updating the nutrients levels

  // if you have nutrients from the worm, then give some nutrients to the Clover
  ifProp EnergyRepresentation.nutrientsType equal 'worm' [[
    prop EnergyRepresentation.nutrientsType setTo 'wormToClover'
    featCall EnergyRepresentation.IU logString 'Nutrients transfer from worm to clover'

    // ‚úèÔ∏è LETS CHANGE THIS: alter how much nutrients the Clover gets from the worm
    prop EnergyRepresentation.nutrientsLevel setTo 0
